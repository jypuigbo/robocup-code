sp {gp*propose*initialize-gp
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <op> + =)
   (<op> ^name initialize-gp)
}

sp {gp*apply*initialize-gp
   (state <s> ^operator.name initialize-gp)
-->
   (<s> ^name gp
        ^localization <l0> <l1> <l2> <l3>
        ^robot <r>
        ^object <obj1>
        ^person <pers1>)
   (<l0> ^id -1)
   (<l1> ^id 0)
   (<l2> ^id 1)
   (<l3> ^id 2)
   (<r> ^locId 0
        ^obj1Id -1
        ^introduced yes -)
   (<obj1> ^id 0
           ^locId 1
           ^found yes -
           ^grasped yes -
           ^delivered yes -
           ^pointed yes -)
   (<pers1> ^id 0
           ^locId 2
           ^near yes -
           ^obj1Id -1
           ^recognized yes -
           ^memorized yes -
           ^askedName yes -)
}
