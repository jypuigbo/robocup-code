grammar bring;

public <basic> = (  (<bringcmd> [<person>] {obj.put("person",$.$value)} [the] <objects> {obj.put("object",$.$value);} ) | <garbage> ) {obj.put("external","1");}; 

<bringcmd> = ((bring) {obj.put("action","bring");});
<person> = me ;
<objects> = orange_juice | energy_drink | seven_up | chocolate_milk | coke | beer_can | fanta | milk | apple_juice;

<garbage>  = <garbage2>;

<garbage1> = _unk;
<garbage2> = <garbage1> | (_unk _unk);
<garbage3> = <garbage2> | (_unk _unk _unk);
<garbage4> = <garbage3> | (_unk _unk _unk _unk);
<garbage5> = <garbage4> | (_unk _unk _unk _unk _unk);


 
