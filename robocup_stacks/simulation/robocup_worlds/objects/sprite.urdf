<robot name="sprite">
	<link name="sprite">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0" />			
			<mass value="0.3" />
			<!-- center of mass (com) is defined w.r.t. link local coordinate system -->
			<origin xyz="0 0 0" />
			<inertia  ixx="0.0" ixy="0.0"  ixz="0.0"  iyy="0.0"  iyz="0.0"  izz="0.0" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.04" length="0.15" />
			</geometry>
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.04" length="0.15" />
			</geometry>
		</collision>
	</link>

	<gazebo reference="sprite">
		<material>Custom/sprite</material>
	</gazebo>
</robot>
